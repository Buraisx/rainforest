<%= link_to "home", products_path%>

<%= form_for @product do |f|%>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<br>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br>
	<%= f.label :url %>
	<%= f.text_field :url %>
	<br>
	<%= f.submit %>
<% end %>